<Page KeepAlive="True"
      x:Class="KnightCrawler.Wpf.InGame"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:KnightCrawler.Wpf"
      xmlns:rendering="clr-namespace:KnightCrawler.Wpf.Rendering"
      mc:Ignorable="d"
      d:DesignHeight="450" d:DesignWidth="800"
      Title="InGame">
    <Grid>
        <rendering:RenderingControl Grid.Row="1" Grid.ColumnSpan="2" RenderOptions.BitmapScalingMode="NearestNeighbor" x:Name="GameRendererPanel" />
    </Grid>
</Page>